<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".JSON_DATA">


    <TextView
        android:id="@+id/txtJSON"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JSON OBJECT DATA"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textSize="20dp"
        android:layout_marginTop="50dp"/>
    <TextView
        android:id="@+id/txtJSONDATA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JSON DATA"
        app:layout_constraintTop_toBottomOf="@+id/txtJSON"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textSize="20dp"
        android:layout_marginTop="50dp"/>

    <TextView
        android:id="@+id/txtJSON2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JSON ARRAY DATA"
        app:layout_constraintTop_toBottomOf="@+id/txtJSONDATA"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textSize="20dp"
        android:layout_marginTop="50dp"/>

    <TextView
        android:id="@+id/txtJSONARRAY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JSON ARRAY DATA"
        app:layout_constraintTop_toBottomOf="@+id/txtJSON2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textSize="20dp"
        android:layout_marginTop="50dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>